// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Email {
  id    Int     @id @default(autoincrement())
  subject String
  sender  String @default("anonymous")
  body    String @default("")
}